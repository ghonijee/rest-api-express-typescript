{
  "name": "be-express-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start:dev": "debug=* ts-node-dev -r tsconfig-paths/register --respawn ./src/main.ts",
    "start": "debug=* ts-node -r tsconfig-paths/register --respawn ./src/main.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "typeorm-ts-node-commonjs migration:create ./src/database/migrations/$npm_config_name",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/utils/database.ts",
    "migration:rollback": "typeorm-ts-node-commonjs migration:revert -d src/utils/database.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "mysql2": "^3.9.1",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.3.20"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^16.11.10",
    "nodemon": "^3.0.3",
    "ts-node": "10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.5.2"
  }
}